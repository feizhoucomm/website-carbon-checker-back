{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/*"] },
      "functions": {
        "index.js": {
          "runtime": "nodejs22.x",
          "maxDuration": 60
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(:path*)",
      "dest": "index.js"
    },
    {
      "src": "/test",
      "dest": "index.js"
    },
    {
      "src": "/carbon",
      "dest": "index.js"
    }
  ]
}
