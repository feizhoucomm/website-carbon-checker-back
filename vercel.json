{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/*"] }
    }
  ],
  "routes": [
    {
      "src": "/(:path*)",
      "dest": "index.js"
    },
    {
      "src": "/test",
      "dest": "index.js"
    },
    {
      "src": "/carbon",
      "dest": "index.js"
    }
  ],
  "functions": [
    {
      "name": "carbon",
      "runtime": "nodejs22.x",
      "config": { "maxDuration": 60 }
    }
  ]
}
